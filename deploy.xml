<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
	<global>
		<version>111</version>
		<id>semanticnotifications</id>
		<vendor>Ontoprise GmbH</vendor>
		<maintainer>Ontoprise GmbH</maintainer>
		<instdir>extensions/SemanticNotifications</instdir>
		<description>
			Sends notifications when values of semantic properties change.
		</description>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:Semantic_Notifications_Extension</helpurl>
		<dependencies>
			<dependency from="1132" to="1160">MW</dependency>
			<dependency from="140" to="152">SMW</dependency>
			<dependency from="142" to="152">smwhalo</dependency>
			<dependency from="100" to="133">SemanticGardening</dependency>
			<dependency from="100" to="100">scriptmanager</dependency>
		</dependencies>
		<patchlevel>0</patchlevel>
	</global>
	<codefiles>
	</codefiles>
	<wikidumps>
		<!-- empty -->
	</wikidumps>
	<resources>
		<!-- empty -->
	</resources>
	<configs>
		<precedes ext="SemanticGardening" />

		<new>
			<require name="semnot"
				file="extensions/SemanticNotifications/includes/SN_Initialize.php" />
			<function name="enableSemanticNotifications">
			</function>
		</new>
		<uninstall>
		</uninstall>
	</configs>
</deploydescriptor>